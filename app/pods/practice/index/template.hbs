<section class="content">
  <div class="container-fluid">
    <div class="block-header">
      <h2>Practice Section</h2>
      <small class="text-muted">Take these challenges and improve your skills </small>
    </div>
    <div class="row clearfix">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card section-card">

          {{#if (not-eq activeContests.length 0)}}
          <div class="header">
            <h2>CATEGORIES</h2>
          </div>
          <div class="body">
            <div class="row clearfix">
              {{#each practiceContests as |contest|}}
                {{#contest-view link='practice-section.problems' contest=contest.contests class="contestCardWrapper"}} {{/contest-view}}
              {{/each}}
            </div>
          </div>
          {{/if}}

        </div>
    </div>
  </div>
</div>
</section>
